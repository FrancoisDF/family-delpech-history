{
	"name": "fusion-svelte-vite-starter",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"ingest:documents": "node scripts/ingest-local-documents.mjs",
		"prepare:rag": "node scripts/ingest-local-documents.mjs",
		"prepare:rag:builder": "node scripts/ingest-builder-content.mjs",
		"prepare:embeddings": "node scripts/precompute-embeddings.mjs",
		"generate:genealogy-tree": "node scripts/generate-genealogy-tree.mjs",
		"parse:mermaid-graph": "node scripts/parse-mermaid-graph.mjs",
		"generate:graphs": "npm run generate:genealogy-tree && npm run parse:mermaid-graph",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"devDependencies": {
		"@builder.io/sdk-svelte": "^5.1.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "2.47.2",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@vitest/browser": "^3.2.3",
		"autoprefixer": "^10.4.16",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^3.4.11",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "npm:rolldown-vite@latest",
		"vitest": "^4.0.0",
		"vitest-browser-svelte": "^0.1.0"
	},
	"overrides": {
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"svelte-preprocess": "^6.0.0"
	},
	"dependencies": {
		"@builder.io/sdk": "^6.1.3",
		"@xenova/transformers": "^2.6.0",
		"js-yaml": "^4.1.1",
		"mammoth": "^1.11.0",
		"pdf-parse": "^2.4.5",
		"valibot": "^1.2.0"
	}
}
